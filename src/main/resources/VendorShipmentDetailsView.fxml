<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" opacity="0.72" prefHeight="400.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rainforestcommerce.rcdb.views.VendorShipmentDetailsViewController">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <children>
            <Text fill="#43a240" strokeType="OUTSIDE" strokeWidth="0.0" text="Shipment" textAlignment="CENTER" wrappingWidth="597.8297576904297">
               <font>
                  <Font name="Trebuchet MS Bold" size="29.0" />
               </font>
            </Text>
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#handleLogoutButtonClicked" text="Logout" StackPane.alignment="CENTER_RIGHT" />
            <Button mnemonicParsing="false" onMouseClicked="#handleVendorButtonClicked" text="&lt;- Vendor" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
      <TableView fx:id="item_table" prefHeight="508.0" prefWidth="600.0">
         <columns>
            <TableColumn editable="false" minWidth="120.0" prefWidth="120.0" text="Product" />
            <TableColumn minWidth="120.0" prefWidth="120.0" text="Size" />
            <TableColumn minWidth="120.0" prefWidth="120.0" text="Brand" />
            <TableColumn editable="false" minWidth="120.0" prefWidth="120.0" text="Price" />
            <TableColumn editable="false" minWidth="120.0" prefWidth="120.0" text="Quantity" />
         </columns>
      </TableView>
      <Button mnemonicParsing="false" onMouseClicked="#handleShipItButtonPressed" text="Ship It" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
